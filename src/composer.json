{
    "name": "alcidesrc/zenrows-web-scrapping-with-php",
    "type": "project",
    "license": "MIT",
    "description": "Web scrapping with PHP",
    "keywords": ["zenrows", "scrapping", "php", "cli", "composer", "phpunit", "paratest", "infection", "phpinsights", "php-parallel-lint", "phpstan", "phpunit", "pcov", "bypass-finals"],
    "authors": [
        {
            "name": "Alcides Ramos",
            "email": "info@alcidesramos.com",
            "homepage": "https://alcidesramos.com/"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.7",
        "voku/simple_html_dom": "^4.8"
    },
    "require-dev": {
        "brianium/paratest": "^6.5",
        "dg/bypass-finals": "^1.4",
        "infection/infection": "^0.26.13",
        "nunomaduro/phpinsights": "^2.4",
        "php-parallel-lint/php-console-color": "^1.0",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTests\\": "tests/Unit",
            "Hooks\\": "tests/Hook"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1.19"
        },
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
